<div class="container">
      <div class="row">
        <div class='col-2'>
        <div class="border">
        <span class="border">
          <p>季節検索</p>
        </span>
        </div>
        <div class="border2">
        <span class="border">
          <tbody>
          <% @seasons.each do |season| %>
          <tr>
            <td>
              <ul>
                <li><%= link_to season.name, cordinates_path(season_id: season.id), class: "text-secondary" %></li>
              </ul>
            </td>
          </tr>
          <% end %>
          </tbody>
        </span>
        </div>
        </div>
      <div class="col-9">
         <h2>コーディネート一覧 (全<%= Cordinate.all.count %>件)</h2>
           <div class="container row">
             <% @cordinates.each do |cordinate| %>
            <div class="pr-5 pb-5 col-3">
              <%= link_to cordinate_path(cordinate) do %>
                <%= image_tag cordinate.get_image(150,100) %>
              <% end %>
               <div><%= cordinate.name %></div>
            </div>
       <% end %>
       </div>
       <%= paginate @cordinates, theme: 'bootstrap-5' %>
     </div>
    </div>
</div>