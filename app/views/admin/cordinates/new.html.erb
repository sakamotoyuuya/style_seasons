<div class="container">
  <div class="row">
    <h3>コーディネート新規登録</h3>
  </div>
  <div class="row">
   <div class="col-3" >
    </div>
    <div class="col-6">
      <%= form_with model:@cordinate,url:admin_cordinates_path do |f| %>
        <table>
          <tbody>
              <tr>
                <td></td>
            　　<td>
            　　</td>
              </tr>
              <tr>
              　<td>コーディネート画像</td>
              　<td>
              　  <%= f.file_field :image, accept: "image/*" %>
              　</td>
              </tr>
              <tr>
                <td>title</td>
                <td>
                  <%= f.text_field :title  %>
                </td>
              </tr>
            <tr>
                <td>季節</td>
              <td>
                 <%= f.collection_select(:season_id, Season.all, :id, :name) %>
              </td>
            </tr
              <tr>
              　<td>コーディネート説明</td>
              　<td>
              　    <%= f.text_area :introduction, rows:'6', placeholder: "コーディネート説明"  %>
              　</td>
              </tr>
              <tr>
                <td>レビュー</td>
                <td>
                  <p></p>
                  <p></p>
                </td>
              </tr>
              <tr>
                <td>
                <%= f.submit '新規登録',class: "btn btn-success" %>
                </td>
              </tr>
          </tbody>
        </table>
        <% end %>
      </div>
    <div class="col-3"></div>
  </div>
</div>